<% if (state === 'contact') { %>
    <script>
        function initMap()
        {
            var sf = {
                lat: 37.76471859999999,
                lng: -122.43194010000002
            }

            var map = new google.maps.Map(document.getElementById(
                'electroIntegrityMap'),
            {
                center: sf,
                zoom: 14
            })

            var marker = new google.maps.Marker(
            {
                position: sf,
                map: map
            })
        }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCdNjb0maBMhvMUc9pYa4sTo6_3lqnxvhI&callback=initMap"
        async defer></script>
    <% } %>
        <% js.forEach(function(src){ %>
