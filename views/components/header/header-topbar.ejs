<div class="top-bar clearfix">
    <div class="container">
        <div class="row">
            <div class="col-xs-8">
                <ul class="contact-info transparent">
                  <li><i class="fa fa-phone"></i> <a href="tel:<%= common.phone.src %>"><%= common.phone.display %></a></li>
                  <li><i class="fa fa-envelope"></i> <a href="mailto:<%= common.email %>"><%= common.email %></a></li>
                </ul>
            </div>
            <div class="col-xs-4 text-right">
                <ul class="social social-minimal">
                  <li><a href="<%= common.social.yelp %>" target="_blank"><i class="fa fa-yelp"></i></a></li>
                  <li><a href="<%= common.social.google %>" target="_blank"><i class="fa fa-google"></i></a></li>
                </ul>
            </div>
        </div>
    </div>
</div>
